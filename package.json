{
  "name": "lo-rez-editor",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write --plugin-search-dir=. src/**/*",
    "build": "npm run build:theme && npm run build:gui",
    "build:gui": "rollup -c",
    "build:theme": "node scripts/build-ui.js",
    "watch:gui": "rollup -c -w",
    "start": "electron main.js"
  },
  "dependencies": {
    "@ibm/plex": "^6.1.1",
    "@rollup/plugin-alias": "^4.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "adm-zip": "^0.5.9",
    "axios": "^1.1.2",
    "carbon-components": "^10.58.3",
    "carbon-components-svelte": "^0.70.10",
    "carbon-icons-svelte": "^11.3.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "electron": "^21.1.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "rollup": "^2.75.6",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "sass": "^1.55.0",
    "svelte": "^3.51.0",
    "svelte-preprocess": "^4.10.7"
  }
}
